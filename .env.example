## Copy this file to .env.local and fill values
# Supabase
SUPABASE_URL=
SUPABASE_KEY=

# Google Gemini API
GEMINI_API_KEY=

# RapidAPI / Judge0
RAPIDAPI_KEY=

# Optional execution token: set to require clients to provide header `x-exec-token`
EXECUTE_API_TOKEN=

# Server
PORT=5000
LOG_LEVEL=info

# Frontend (Vite)
VITE_SENTRY_DSN=
# GenSpark Environment Variables

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Razorpay Configuration
VITE_RAZORPAY_KEY_ID=your_razorpay_key_id
VITE_RAZORPAY_KEY_SECRET=your_razorpay_key_secret
VITE_RAZORPAY_WEBHOOK_SECRET=your_razorpay_webhook_secret
VITE_RAZORPAY_PLAN_ID=your_razorpay_plan_id

# Google Gemini API
VITE_GEMINI_API_KEY=your_gemini_api_key

# Judge0 API (optional, for premium compiler service)
VITE_RAPIDAPI_KEY=your_rapidapi_key