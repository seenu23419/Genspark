# API Base URL (if using a backend proxy or server)
VITE_API_URL=http://localhost:5000

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Gemini AI API Key
VITE_GEMINI_API_KEY=your_gemini_api_key

# Code Execution - Judge0 Self-Hosted
# Set this to your self-hosted Judge0 URL (e.g., http://localhost:2358)
# Leave empty to use Piston (public API) as fallback
VITE_JUDGE0_URL=http://localhost:2358

# Code Execution - RapidAPI Judge0 (fallback, requires API key)
# Get key from https://rapidapi.com/judge0-official/api/judge0
VITE_RAPIDAPI_KEY=your_rapidapi_key

